    <style name="AlertDialogTheme" parent="ThemeOverlay.MaterialComponents.MaterialAlertDialog">
        <item name="materialAlertDialogTitleTextStyle">@style/TitleTextStyle</item>
        <item name="materialAlertDialogBodyTextStyle">@style/BodyTextStyle</item>
        <item name="buttonBarPositiveButtonStyle">@style/PositiveButtonStyle</item>
        <item name="buttonBarNegativeButtonStyle">@style/NegativeButtonStyle</item>
    </style>
    <style name="BodyTextStyle" parent="MaterialAlertDialog.MaterialComponents.Body.Text">
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/_12sdp</item>
        <item name="fontFamily">@font/medium</item>
    </style>
    <style name="DialogTheme.Fullscreen" parent="Theme.AxisSeedsAndCrop">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowFullscreen">false</item>
        <item name="android:windowIsFloating">false</item>
    </style>
    <style name="NegativeButtonStyle" parent="Widget.MaterialComponents.Button.TextButton.Dialog">
        <item name="android:textColor">@color/green</item>
        <item name="rippleColor">@color/black</item>
        <item name="fontFamily">@font/bold</item>
    </style>
    <style name="PositiveButtonStyle" parent="Widget.MaterialComponents.Button.TextButton.Dialog">
        <item name="android:textColor">@color/green</item>
        <item name="rippleColor">@color/black</item>
        <item name="fontFamily">@font/bold</item>
    </style>
    <style name="TextStyleBody" parent="Widget.MaterialComponents.TextView">
        <item name="fontFamily">@font/regular</item>
        <item name="android:fontFamily">@font/regular</item>
        <item name="android:textSize">@dimen/_12sdp</item>
        <item name="android:includeFontPadding">false</item>
        <item name="android:textColor">@color/black</item>
    </style>
    <style name="TextStyleTitle" parent="Widget.MaterialComponents.TextView">
        <item name="fontFamily">@font/medium</item>
        <item name="android:fontFamily">@font/medium</item>
        <item name="android:textSize">@dimen/_16sdp</item>
        <item name="android:includeFontPadding">false</item>
        <item name="android:textColor">@color/black</item>
    </style>
    <style name="Theme.AxisSeedsAndCrop" parent="Theme.MaterialComponents.Light.NoActionBar">
        
        <item name="colorPrimary">@color/green</item>
        <item name="colorPrimaryVariant">@color/green</item>
        <item name="colorOnPrimary">@color/white</item>
        
        <item name="colorSecondary">@color/green</item>
        <item name="colorSecondaryVariant">@color/green</item>
        <item name="colorOnSecondary">@color/black</item>
        
        <item name="android:statusBarColor" ns1:targetApi="l">@color/status_bar</item>
        <item name="android:windowLightStatusBar" ns1:targetApi="m">true</item>
        
        <item name="android:actionMenuTextColor">@color/green</item>
        <item name="android:fontFamily">@font/medium</item>
    </style>
    <style name="TitleTextStyle" parent="MaterialAlertDialog.MaterialComponents.Title.Text">
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/_12sdp</item>
        <item name="fontFamily">@font/bold</item>
    </style>
